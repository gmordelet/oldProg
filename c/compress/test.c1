#include<stdio.h>

int main(int argc, char *argv[])
{
  FILE *fp1, *fp2;
  int c1, c2;
  unsigned long byte;
  if(argc != 3)
    return -1;
  fp1 = fopen(argv[1], "r");
  fp2 = fopen(argv[2], "r");
  byte = 1;
  while((c1 = fgetc(fp1)) == (c2 = fgetc(fp2)) && c1 != EOF)
    ++byte;
  if(c1 == c2)
    printf("Fichiers identiques\n");
  else
    printf("Fichiers diff√©rents en byte %ld\n", byte);
  return 0;
}
